{
    "name": "pubsubjs",
    "version": "0.1.0",
    "author": "M4RC3L05 <joaobat@live.com.pt>",
    "license": "MIT",
    "scripts": {
        "build:web:all": "yarn build:web:prod && build:web:dev",
        "build:web:prod": "yarn ts:web && yarn webpack:web:prod",
        "build:web:dev": "yarn ts:web && yarn webpack:web:dev",
        "webpack:web:dev": "webpack --mode production --config .webpack/webpack.web.dev.config.js",
        "webpack:web:prod": "webpack --mode production --config .webpack/webpack.web.prod.config.js",
        "ts:web": "tsc --p .tsconfig/tsconfig.web.json",
        "ts:node": "tsc --p .tsconfig/tsconfig.node.json"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "babel-loader": "^8.0.4",
        "ts-loader": "^5.3.2",
        "typescript": "^3.2.2",
        "webpack": "^4.28.2",
        "webpack-cli": "^3.1.2"
    }
}
